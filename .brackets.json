{
    "path": {
        "*.md": {
            "spaceUnits": 2,
            "wordWrap": true,
            "brackets-git.gitPath": "git"
        },
        "*.js": {
            "spaceUnits": 2,
            "wordWrap": true,
            "brackets-git.gitPath": "/usr/local/bin/git",
            "fileFilters": [
                {
                    "name": "Gatsby |Â SCSS",
                    "patterns": [
                        "*.map"
                    ]
                }
            ],
            "jwolfe.file-tree-exclude.list": [
                "node_modules",
                "bower_components",
                ".git",
                "dist",
                "vendor"
            ]
        },
        "*.scss": {
            "spaceUnits": 4,
            "brackets-git.gitPath": "git"
        },
        "*.css": {
            "spaceUnits": 4
        }
    },
    "wordWrap": false,
    "spaceUnits": 4,
    "language.fileExtensions": {
        "js": "jsx"
    },
    "closeTags": {
        "dontCloseTags": [],
        "indentTags": [],
        "whenClosing": true,
        "whenOpening": true
    },
    "brackets-indent-guides.guideColor": "rgba(100, 100, 100, 0.3)"
}
