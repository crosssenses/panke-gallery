{
    "path": {
        "*.md": {
            "spaceUnits": 2,
            "wordWrap": true,
            "brackets-git.gitPath": "git"
        },
        "*.js": {
            "spaceUnits": 2,
            "wordWrap": true,
            "brackets-git.gitPath": "/usr/local/bin/git"
        },
        "*.scss": {
            "spaceUnits": 4,
            "brackets-git.gitPath": "git"
        },
        "*.css": {
            "spaceUnits": 4
        }
    },
    "wordWrap": false,
    "spaceUnits": 4,
    "language.fileExtensions": {
        "js": "jsx"
    }
}
