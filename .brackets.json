{
    "path": {
        "*.md": {
            "spaceUnits": 2,
            "wordWrap": true,
            "brackets-git.gitPath": "git"
        },
        "*.js": {
            "spaceUnits": 2,
            "wordWrap": true,
            "brackets-git.gitPath": "/usr/local/bin/git",
            "fileFilters": [
                {
                    "name": "public+cache+map",
                    "patterns": [
                        "public/*",
                        ".cache/*",
                        "node-modules/*",
                        "*.map"
                    ]
                },
                {
                    "name": "Graphite-only",
                    "patterns": [
                        "docs/*",
                        "styles/bootstrap/*"
                    ]
                }
            ],
            "jwolfe.file-tree-exclude.list": [
                "node_modules",
                "bower_components",
                ".git",
                "dist",
                "vendor"
            ],
            "brackets-git.terminalCommand": "/Users/larissa/Library/Application Support/Brackets/extensions/user/zaggino.brackets-git/shell/terminal.osa",
            "linting.collapsed": true,
            "linting.enabled": true,
            "quickview.enabled": true
        },
        "*.scss": {
            "spaceUnits": 4,
            "brackets-git.gitPath": "/usr/bin/git"
        }
    },
    "wordWrap": false,
    "spaceUnits": 2,
    "language.fileExtensions": {
        "js": "jsx"
    },
    "closeTags": {
        "dontCloseTags": [],
        "indentTags": [],
        "whenClosing": true,
        "whenOpening": true
    },
    "brackets-indent-guides.guideColor": "rgba(100, 100, 100, 0.3)"
}
